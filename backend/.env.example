# Backend example env file
# Copy to .env and fill values in development

# Database
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name
DB_HOST=localhost

# App
NODE_ENV=development
PORT=5000
HOST=0.0.0.0
JWT_SECRET=your_jwt_secret

# Frontend origins (comma separated)
FRONTEND_URL=http://localhost:3000
# Optional: comma-separated list of allowed origins (used by CORS).
# Example: http://localhost:3000,http://127.0.0.1:3000
FRONTEND_ORIGINS=http://localhost:3000

# Ultravox API
ULTRAVOX_API_URL=https://api.ultravox.ai/api/calls
ULTRAVOX_API_KEY=your_ultravox_api_key

# Optional cookie domain for cross-subdomain cookies
COOKIE_DOMAIN=

# Trust proxy (set to '1' or 'true' when behind a reverse proxy)
TRUST_PROXY=
